package ex1_4;

import ex1_3.MyPoint;

public class MyCircle {

	private MyPoint center;
	private int radius;

	public MyCircle(int x, int y, int radius) {
		this.center = new MyPoint(x, y);
		this.radius = radius;
	}

	public MyCircle(MyPoint center, int radius) {
		this.center = center;
		this.radius = radius;
	}
}
